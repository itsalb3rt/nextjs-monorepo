{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2022,
      "ecmaFeatures": {
          "jsx": true,
          "modules": true
      }
  },
  "settings": {
    "react": {
        "version": "detect"
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "single"],
      "react/prop-types": ["off"]
  }
}